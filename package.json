{
  "name": "sql-workbench-embedded",
  "version": "0.1.0",
  "description": "Lightweight JavaScript library that transforms static SQL code blocks into interactive, browser-based SQL execution environments using DuckDB WASM",
  "main": "dist/sql-workbench-embedded.js",
  "browser": "dist/sql-workbench-embedded.js",
  "module": "dist/sql-workbench-embedded.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/sql-workbench-embedded.js",
  "jsdelivr": "dist/sql-workbench-embedded.js",
  "exports": {
    ".": {
      "import": "./dist/sql-workbench-embedded.esm.js",
      "require": "./dist/sql-workbench-embedded.js",
      "script": "./dist/sql-workbench-embedded.js",
      "default": "./dist/sql-workbench-embedded.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist/ && tsc && vite build && npm run copy:example",
    "build:prod": "NODE_ENV=production npm run build",
    "copy:example": "cp examples/index.html dist/ && sed -i.bak 's|../src/index.ts|../sql-workbench-embedded.esm.js|' dist/index.html && rm -f dist/index.html.bak",
    "preview": "vite preview",
    "preview:prod": "npm run build:prod && vite preview",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:prod"
  },
  "keywords": [
    "sql",
    "duckdb",
    "wasm",
    "embed",
    "embedded",
    "embeddable",
    "interactive",
    "sql-editor",
    "sql-workbench",
    "documentation",
    "tutorial",
    "education",
    "queries",
    "in-browser"
  ],
  "author": "TobiLG <tobilg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobilg/sql-workbench-embedded/issues"
  },
  "homepage": "https://github.com/tobilg/sql-workbench-embedded#readme",
  "devDependencies": {
    "@duckdb/duckdb-wasm": "^1.31.1-dev1.0",
    "@types/node": "^22.18.12",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.3"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "^1.31.1-dev1.0"
  },
  "peerDependenciesMeta": {
    "@duckdb/duckdb-wasm": {
      "optional": true
    }
  }
}
